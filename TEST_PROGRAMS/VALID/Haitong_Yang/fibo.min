#computer the nth fibonacci number

var a : int;
var b : int;
var c : int;
var n : int;

#n should be greater than zero, but no way to check because don't have '>' or '<'
n = 10;
a=1;
b=1;

if (n-1) then
        if(n-2) then
                while(n-2) do
                        c=a+b;
                        a=b;
                        b=c;
                        n = n-1;
                done
                print b;
        else
                print "1";
        endif
else
        print "1";
endif
