# Compute Fib(x) where x is read
var x: int;
var temp: int;
var first: int;
var second: int;

print "Please enter the number of the fibonanci that you want to calculate";
read x;
first=1;
second = 1;
x = x -2;
while x do
 	temp = first;
   first = second;
   second = (temp + second);
   x = x - 1;
done

print "The Result of your fibonanci computation is";
print second;

