# Reads a specified number of integers from user and prints the largest.

var max : int;
var attemps : int;
var temp : int;

max = -999;
attempts = 5; # Specify how many inputs to take in
temp = max;

while attempts do
	read temp;
	if (temp - max) then
		max = temp;
	else
		max = max;
	endif

	attempts = attempts - 1;
done

print max;
